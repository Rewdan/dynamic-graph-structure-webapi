@model IEnumerable<DynamicGraphStructure.WebApi.Database.Models.AlgorithmIO>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeIO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Props)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Index)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsNecesse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Algorithm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Function)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Props)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Index)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsNecesse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Algorithm.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Function.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
